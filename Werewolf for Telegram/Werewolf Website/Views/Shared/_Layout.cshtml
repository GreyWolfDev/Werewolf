
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="description" content="@ViewBag.Description" />
    <meta name="keywords" content="werewolf, ultimate werewolf, telegram, moderator, para949" />
    <meta name="robots" content="index,follow" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta property="og:title" content="@ViewBag.Title" />
    <meta property="og:description" content="@ViewBag.Description" />
    <meta property="og:url" content="http://www.tgwerewolf.com" />
    @if (ViewBag.ImagePath != null)
    {
        <meta property="og:image" content="@ViewBag.ImagePath" />
    }
    else
    {
        <meta property="og:image" content="/images/mob_banner 2.jpg" />
    }
    <title>@ViewBag.Title - Werewolf for Telegram</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!-- mobile settings -->
    <meta name="viewport" content="width=device-width, maximum-scale=1, initial-scale=1, user-scalable=0" />
    <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->
    <!-- WEB FONTS : use %7C instead of | (pipe) -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400%7CRaleway:300,400,500,600,700%7CLato:300,400,400italic,600,700" rel="stylesheet" type="text/css" />

    <!-- CORE CSS -->
    <link href="~/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />

    <!-- THEME CSS -->
    <link href="~/Content/essentials.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/layout.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/layout-dark.css" rel="stylesheet" type="text/css" name="dark" />

    <!-- PAGE LEVEL SCRIPTS -->
    <link href="~/Content/header-1.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/color_scheme/red.css" rel="stylesheet" type="text/css" id="color_scheme" />
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script type="application/ld+json">
        {
        "@@context": "http://schema.org",
        "@@type": "Organization",
        "name": "Werewolf Telegram",
        "alternateName": "Play Werewolf on Telegram with your friends",
        "url": "http://www.tgwerewolf.com",
        "logo": "http://www.tgwerewolf.com/Images/werewolf.jpg",
        "sameAs": [
        "https://twitter.com/tgwerewolf"
        ],
        "founder": {
        "@@type": "Person",
        "name": "Para"
        }
        }
    </script>
    @if (Request.Url.AbsoluteUri.Contains("parawuff.com"))
    {
        Response.Status = "301 Moved Permanently";
        var newUri = Request.Url.AbsoluteUri.Replace("werewolf", "www").Replace("parawuff.com", "tgwerewolf.com");
        Response.AddHeader("Location", newUri);
        Response.StatusCode = 301;
    }
    <!-- JAVASCRIPT FILES -->

    <script>
        $(document).ready(function () {
            _misc();
        });
    </script>
