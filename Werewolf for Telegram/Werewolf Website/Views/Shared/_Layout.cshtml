        <div id="header" class="sticky dark clearfix">
            <header id="topNav">
                <div class="container">
                    <!-- Mobile Menu Button -->
                    <button class="btn btn-mobile" data-toggle="collapse" data-target=".nav-main-collapse">
                        <i class="fa fa-bars"></i>
                    </button>
                    <!-- Logo -->

                    <a href="#topNav" class="logo pull-left scrollTo">
                        <img src="/images/werewolf title.png" style="width: 20em; height: auto;" alt="" />
                    </a>

                    <div class="navbar-collapse pull-right nav-main-collapse collapse">
                        <nav class="nav-main">
                            <ul id="topMain" class="nav nav-pills nav-main nav-onepage">

                                <li><a href="/Home/Index">Home</a></li>
                                <li><a href="/Stats/Index">Stats</a></li>
                            </ul>
                            @*@Html.Partial("_LoginPartial")*@
                        </nav>
                    </div>
                </div>
            </header>
        </div>



        @RenderBody()

        <!-- SCROLL TO TOP -->
        <a href="#" id="toTop"></a>


        <!-- PRELOADER -->
        <div id="preloader">
            <div class="inner">
                <span class="loader"></span>
            </div>
        </div><!-- /PRELOADER -->



        <footer id="footer">
            <div class="copyright">
                <div class="container">
                    <ul class="pull-right nomargin list-inline mobile-block">
                        <li><a href="https://github.com/parabola949/Werewolf">GitHub</a></li>
                        <li>&bull;</li>
                        <li><a href="https://telegram.me/werewolfbot">@@werewolfbot</a></li>
                    </ul>
                    &copy; @DateTime.Now.Year - Werewolf for Telegram
                </div>
            </div>

        </footer>
    </div>

    @try
    {
        RenderSection("scripts", required: false);
    }
    finally
    {
        
    }
